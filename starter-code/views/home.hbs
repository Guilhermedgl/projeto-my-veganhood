
<div id="map"></div>
<div id="list" class="d-flex justify-content-center">
    <div id="list-rest">
        <h2>Restaurants</h2>
        {{#each Rest}}
            <h3><a href="#" class="draw">{{this.Name}}</a></h3>
            <p>{{this.Address}}</p>
            <p>{{this.Phone}}</p>
            <hr>
            {{else}}
            <p>no content</p>
        {{/each}}
    </div>
</div>

<!--  Axios CDN  -->
